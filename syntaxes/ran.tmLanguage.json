{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "RAN",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.ran",
					"match": "\\b(if|endif|while|endwhile|for|endfor|return|def|enddef|modname|import|export)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.ran",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ran",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.ran"
}